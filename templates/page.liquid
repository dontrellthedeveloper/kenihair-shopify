{% comment %}
{% layout 'theme-2' %}
{% endcomment %}

  {% if page.title != blank %}
    <div class="section-header text-center testing class" style="background-image: url('{{ 'keni-gradient-bg-lg-9.jpg' | asset_img_url: 'master' }}'); padding: 30px 0 30px 0; border: 1px solid #444; opacity: 0.6;">
      <h1 style="margin: auto auto; color: #000;   text-shadow: 2px 2px 4px #fff;">{{ page.title | escape }}</h1>
    </div>
  {% endif %}

<div class="page-width">
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      
      {% comment %}
      <div class="section-header text-center">
        <h1>{{ page.title }}</h1>
      </div>
	  {% endcomment %}
      
      <div class="rte">
        {{ page.content }}
      </div>
    </div>
  </div>
</div>



{% comment %}

  {% if section.settings.title != blank %}
    <div class="section-header text-center testing class" style="background-image: url('{{ 'keni-gradient-bg-lg-7.jpg' | asset_img_url: 'master' }}'); padding: 30px 0 30px 0; border: 1px solid #444;">
      <h2 style="margin: auto auto;">{{ section.settings.title | escape }}</h2>
    </div>
  {% endif %}

{% endcomment %}